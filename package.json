{
  "name": "ttr-server",
  "version": "1.0.0",
  "private": true,
  "description": "Made using [create-react-app](https://github.com/facebookincubator/create-react-app)",
  "main": "server/server.js",
  "dependencies": {
    "babel-core": "6.14.0",
    "bcrypt": "0.8.7",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "express": "4.13.3",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "pg": "^7.4.0",
    "redux-mock-store": "^1.3.0",
    "request": "^2.83.0",
    "superagent": "^3.8.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "chai-http": "^3.0.0",
    "chai-param": "^0.1.2",
    "concurrently": "3.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "fetch-mock": "^5.13.1",
    "really-need": "^1.9.2",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "server": "node server/server.js",
    "client": "node start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "test": "nyc mocha",
    "doc": "apidoc -e node_modules",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "**/*.js",
      "**/*.jsx"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js",
      "coverage/**",
      "**/test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/__specs__/**",
      "**/containers/**",
      "**/components/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saellis/fuzzy-meme.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saellis/fuzzy-meme/issues"
  },
  "homepage": "https://github.com/saellis/fuzzy-meme#readme"
}
